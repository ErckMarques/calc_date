{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "debug_formatter": {
            "format": "%(asctime)s.%(msecs)03d - %(name)s - %(levelname)s - %(filename)s:%(lineno)d - %(funcName)s() - %(message)s"
        },
        "json": {
            "()": "date_calc.log.formatters.JSONLogFormatter",
            "include_keys": [
                "created",
                "message",
                "levelname",
                "name",
                "filename",
                "module",
                "exc_info",
                "lineno",
                "threadName",
                "processName",
                "taskName",
                "args",
                "contexto"
            ],
            "format": "%(asctime)s.%(msecs)03d - %(name)s - %(levelname)s - %(filename)s:%(lineno)d - %(funcName)s(%(args)s)- %(message)s  [contexto: %(contexto)s]"
        }
    },
    "filters": {
        "()": "date_calc.log.filters.ContextFilter"
    },
    "handlers": {},
    "root": {},
    "loggers": {
        "date_calc": {"level": "DEBUG"}
    }
}
